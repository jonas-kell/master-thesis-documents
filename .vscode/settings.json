{
    "spellright.language": ["English (American)"],
    "spellright.documentTypes": ["markdown", "latex", "plaintext"],
    "files.exclude": {
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.log": true,
        "**/*.aux": true,
        "**/*.bcf": true,
        "**/*.out": true,
        "**/*.run.xml": true,
        "**/*.synctex.gz": true,
        "**/*.synctex(busy)": true,
        "**/*.toc": true,
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*-blx.bib": true,
        "**/*.nav": true,
        "**/*.snm": true,
        "project-work-presentation/beamerthemeuniamntf.sty": true,
        "project-work-presentation/beamercolorthemeuniamntf.sty": true,
        "project-work-presentation/slide-background-images": true,
        "**/_minted*": true,
        "**/*.minted": true,
        "thesis-latex/plotgeneration/**/*.json": true,
        "thesis-latex/plotgeneration/**/*.csv": true
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "--shell-escape", // added this flag
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape", // added this flag
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": ["%DOCFILE%"],
            "env": {}
        }
    ],
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "cSpell.words": [
        "aggregatorenv",
        "ALCC",
        "Ansatz",
        "approximators",
        "Bosonic",
        "cumulant",
        "cumulants",
        "diagonalization",
        "experimentenv",
        "exponentials",
        "fermionic",
        "Fubini",
        "Hamiltonians",
        "Heun's",
        "Heyl",
        "hpcaugsburg",
        "kelljona",
        "Kolumnentitel",
        "licca",
        "Markus",
        "miniforge",
        "mult",
        "myenv",
        "Noether's",
        "numpy",
        "parametrizations",
        "sbatch",
        "scancel",
        "scipy",
        "slurm",
        "squeue",
        "TDVP",
        "unviable",
        "Venv"
    ]
}
